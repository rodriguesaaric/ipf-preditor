{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/predict",
            "dest": "/api/predict.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
